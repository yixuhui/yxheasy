cordova plugin add cordova-plugin-baidumaplocation --variable ANDROID_KEY="QOYFQk4E5RQPQvOEkiQCQpKStlhIPx21" --variable IOS_KEY="QOYFQk4E5RQPQvOEkiQCQpKStlhIPx21"
